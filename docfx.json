{
  "metadata": [
    {
      "src": [
        {
          "files": [ "DevicesSyncUnity.csproj" ]
        }
      ],
      "dest": "obj/api",
      "filter": "docs/filterConfig.yml"
    }
  ],
  "build": {
    "globalMetadata":
    {
      "_appTitle": "DevicesSyncUnity documentation",
      "_appFooter": "<a href=\"https://github.com/enormand/DevicesSyncUnity\">DevicesSyncUnity</a> - <a href=\"https://twitter.com/NormandErwan\">Erwan Normand</a>",
      "_enableSearch": true
    },
    "content": [
      {
        "src": "obj/api",
        "dest": "api",
        "files": [ "*.yml" ]
      },
      {
        "src": "docs/",
        "files": [ "toc.yml", "index.md", "LICENSE" ]
      }
    ],
    "xref": [ "https://raw.githubusercontent.com/enormand/docfx-unity/master/xrefmap.yml" ],
    "dest": "docs/"
  }
}